// Place your settings in this file to overwrite default and user settings.
{
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/.DS_Store": true,
    "node_modules": true,
    ".vs": true,
    "**/bin": true,
    "**/obj": true,
    "**/project.lock.json": true,
    "**/*.xproj*": true,
    "build/**": true,
    "coverage/**": true,
    "*.sln.DotSettings.user": true,
    "package-lock.json": true,
    "yarn.lock": true,
    "nscreg.sln.DotSettings": true
  },
  "vsicons.presets.angular": false,
  "gitlens.codeLens.enabled": false,
  "flow.enabled": false,
  "path-intellisense.mappings": {
    "components": "${workspaceRoot}/client/components",
    "helpers": "${workspaceRoot}/client/helpers",
    "layout": "${workspaceRoot}/client/layout",
    "pages": "${workspaceRoot}/clientpages/"
  },
  "npm-intellisense.scanDevDependencies": true,
  "eslint.autoFixOnSave": true
}
