{
  "name": "nscreg",
  "author": "TimelySoft LLC",
  "description": "nscreg clinet scripts",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=6.9",
    "npm": ">=3.10"
  },
  "scripts": {
    "postinstall": "dotnet restore",
    "eslint": "eslint client client.test run.js webpack.config.js",
    "eslint-tfs": "eslint client client.test run.js webpack.config.js -f visualstudio",
    "stylelint": "stylelint \"client/components/**/*.pcss\" \"client/pages/**/*.pcss\"",
    "lint": "run-p eslint stylelint",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "node run clean",
    "build": "node run build",
    "build:debug": "node run build --debug",
    "start": "node run"
  },
  "repository": {
    "type": "git",
    "url": "http://ci2.timelysoft.org:8080/tfs/defaultcollection/_git/nscreg"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "fastclick": "1.0.6",
    "griddle-react": "^1.1.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "ramda": "^0.23.0",
    "react": "^15.4.2",
    "react-datepicker": "^0.41.1",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.6.0",
    "redux-act": "^1.1.1",
    "redux-thunk": "^2.2.0",
    "semantic-ui-react": "^0.67.2",
    "yup": "^0.21.3"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel-core": "^6.24.0",
    "babel-eslint": "^7.2.1",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-class-properties": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-preset-jest": "^19.0.0",
    "babel-preset-latest": "^6.24.0",
    "babel-preset-react": "^6.23.0",
    "browser-sync": "^2.18.8",
    "cpy": "^5.0.0",
    "cross-env": "^3.2.4",
    "css-loader": "^0.27.3",
    "del": "^2.2.2",
    "enzyme": "^2.8.0",
    "eslint": "^3.18.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-fetch": "0.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "eventsource-polyfill": "^0.9.6",
    "file-loader": "^0.10.1",
    "jest": "^19.0.2",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.0.2",
    "postcss": "^5.2.16",
    "postcss-cssnext": "^2.10.0",
    "postcss-easy-import": "^2.0.0",
    "postcss-flexibility": "^1.1.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.3",
    "postcss-nested-props": "^1.1.7",
    "precss": "^1.4.0",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "react-test-renderer": "^15.4.2",
    "style-loader": "^0.16.0",
    "stylelint": "^7.9.0",
    "stylelint-config-css-modules": "^0.1.0",
    "stylelint-config-standard": "^16.0.0",
    "sugarss": "^0.2.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.2",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.17.1"
  },
  "babel": {
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread"
    ],
    "presets": [
      [
        "latest",
        {
          "es2015": {
            "modules": false
          }
        }
      ],
      "react"
    ],
    "env": {
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs"
        ]
      }
    }
  },
  "postcss": {
    "parser": "sugarss",
    "plugins": {
      "postcss-easy-import": {
        "extensions": [
          ".pcss"
        ]
      },
      "postcss-import": {},
      "precss": {},
      "postcss-cssnext": {},
      "postcss-flexibility": {},
      "postcss-nested-props": {}
    }
  },
  "eslintConfig": {
    "root": true,
    "parser": "babel-eslint",
    "plugins": [
      "babel",
      "fetch",
      "jsx-a11y",
      "import",
      "react"
    ],
    "settings": {
      "import/resolver": "webpack"
    },
    "env": {
      "browser": true,
      "es6": true,
      "jest": true
    },
    "extends": "airbnb",
    "rules": {
      "import/no-extraneous-dependencies": 0,
      "linebreak-style": 0,
      "no-confusing-arrow": 0,
      "no-nested-ternary": 0,
      "no-plusplus": 0,
      "semi": [
        2,
        "never"
      ]
    }
  },
  "stylelint": {
    "extends": [
      "stylelint-config-css-modules",
      "stylelint-config-standard"
    ],
    "rules": {
      "declaration-block-trailing-semicolon": null,
      "string-quotes": "single"
    }
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "cobertura"
    ],
    "collectCoverageFrom": [
      "client/**/*.{js,jsx}"
    ],
    "testEnvironment": "node",
    "testRegex": "/client.test/.*|(\\.|/)(test|spec)\\.jsx?$",
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules)[/\\\\]"
    ],
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest"
    }
  }
}
