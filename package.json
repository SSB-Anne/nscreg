{
  "name": "nscreg",
  "author": "TimelySoft LLC",
  "description": "nscreg clinet scripts",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": ">=6.9",
    "npm": ">=3.10"
  },
  "scripts": {
    "postinstall": "dotnet restore",
    "eslint": "eslint client client.test run.js webpack.config.js",
    "eslint-tfs": "eslint client client.test run.js webpack.config.js -f visualstudio",
    "stylelint": "stylelint \"client/components/**/*.pcss\" \"client/pages/**/*.pcss\"",
    "lint": "run-p eslint stylelint",
    "test": "jest",
    "test:watch": "jest --watch",
    "clean": "node run clean",
    "build": "node run build",
    "build:debug": "node run build --debug",
    "start": "node run"
  },
  "repository": {
    "type": "git",
    "url": "http://ci2.timelysoft.org:8080/tfs/defaultcollection/_git/nscreg"
  },
  "dependencies": {
    "babel-polyfill": "^6.23.0",
    "fastclick": "1.0.6",
    "griddle-react": "^1.1.0",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.4",
    "moment": "^2.17.1",
    "ramda": "^0.23.0",
    "react": "^15.4.2",
    "react-datepicker": "^0.41.1",
    "react-dom": "^15.4.2",
    "react-formal": "^0.24.3",
    "react-redux": "^5.0.4",
    "react-router": "3.0.3",
    "react-router-redux": "^4.0.8",
    "recompose": "^0.23.1",
    "redux": "^3.6.0",
    "redux-act": "^1.1.1",
    "redux-thunk": "^2.2.0",
    "reselect": "^3.0.0",
    "semantic-ui-react": "^0.68.0",
    "yup": "^0.21.3"
  },
  "devDependencies": {
    "assets-webpack-plugin": "^3.5.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-module-resolver": "^2.7.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-jest": "^19.0.0",
    "babel-preset-react": "^6.24.1",
    "browser-sync": "^2.18.8",
    "browserslist": "^1.7.7",
    "cpy": "^5.0.0",
    "cross-env": "^4.0.0",
    "css-loader": "^0.28.0",
    "del": "^2.2.2",
    "enzyme": "^2.8.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-import-resolver-babel-module": "^3.0.0",
    "eslint-import-resolver-webpack": "^0.8.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-compat": "^1.0.2",
    "eslint-plugin-fetch": "0.0.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "eventsource-polyfill": "^0.9.6",
    "file-loader": "^0.11.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^19.0.2",
    "mkdirp": "^0.5.1",
    "npm-run-all": "^4.0.2",
    "postcss": "^5.2.17",
    "postcss-cssnext": "^2.10.0",
    "postcss-easy-import": "^2.0.0",
    "postcss-flexibility": "^1.1.1",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.3",
    "postcss-nested-props": "^1.1.7",
    "precss": "^1.4.0",
    "prop-types": "^15.5.8",
    "react-addons-perf": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "style-loader": "^0.16.1",
    "stylelint": "^7.10.1",
    "stylelint-config-css-modules": "^1.0.0",
    "stylelint-config-standard": "^16.0.0",
    "sugarss": "^0.2.0",
    "url-loader": "^0.5.8",
    "webpack": "^2.4.1",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.0"
  },
  "babel": {
    "plugins": [
      [
        "transform-class-properties"
      ],
      [
        "transform-object-rest-spread"
      ],
      [
        "module-resolver",
        {
          "root": [
            "."
          ],
          "alias": {
            "components": "./client/components",
            "helpers": "./client/helpers",
            "layout": "./client/layout",
            "pages": "./client/pages"
          }
        }
      ]
    ],
    "presets": [
      [
        "env",
        {
          "es2015": {
            "modules": false
          }
        }
      ],
      "react"
    ],
    "env": {
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs"
        ]
      }
    }
  },
  "postcss": {
    "parser": "sugarss",
    "plugins": {
      "postcss-easy-import": {
        "extensions": [
          ".pcss"
        ]
      },
      "postcss-import": {},
      "precss": {},
      "postcss-cssnext": {},
      "postcss-flexibility": {},
      "postcss-nested-props": {}
    }
  },
  "eslintConfig": {
    "root": true,
    "parser": "babel-eslint",
    "plugins": [
      "babel",
      "fetch",
      "jsx-a11y",
      "import",
      "react",
      "compat"
    ],
    "settings": {
      "import/resolver": {
        "babel-module": {}
      }
    },
    "env": {
      "browser": true,
      "es6": true,
      "jest": true
    },
    "extends": "airbnb",
    "rules": {
      "import/no-extraneous-dependencies": 0,
      "linebreak-style": 0,
      "no-confusing-arrow": 0,
      "no-nested-ternary": 0,
      "no-plusplus": 0,
      "semi": [
        2,
        "never"
      ]
    }
  },
  "stylelint": {
    "extends": [
      "stylelint-config-css-modules",
      "stylelint-config-standard"
    ],
    "rules": {
      "no-extra-semicolons": true,
      "declaration-block-semicolon-newline-after": "never-multi-line",
      "declaration-block-semicolon-newline-before": "never-multi-line",
      "declaration-block-semicolon-space-after": "never",
      "declaration-block-semicolon-space-before": "never",
      "declaration-block-trailing-semicolon": "never",
      "string-quotes": "single"
    }
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "cobertura"
    ],
    "collectCoverageFrom": [
      "client/**/*.{js,jsx}"
    ],
    "moduleNameMapper": {
      "^components$": "<rootDir>/client/components/",
      "^helpers$": "<rootDir>/client/helpers/",
      "^layout$": "<rootDir>/client/layout/",
      "^.+\\.(css|pcss)$": "<rootDir>/csstub.js",
      "^./styles$": "<rootDir>/cssstub.js"
    },
    "testEnvironment": "node",
    "testRegex": "/client.test/.*|(\\.|/)(test|spec)\\.jsx?$",
    "testPathIgnorePatterns": [
      "<rootDir>[/\\\\](build|docs|node_modules)[/\\\\]"
    ],
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ],
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest"
    }
  },
  "browserslist": {
    "production": [
      "last 3 version"
    ],
    "development": [
      "last 1 version"
    ]
  }
}
