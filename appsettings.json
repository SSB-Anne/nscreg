{
  "ConnectionString": "Server=;Port=;Database=;User Id=;Password=",
  "UseInMemoryDatabase": false,
  "StatUnitAnalysisServiceDequeueInterval": 9999,
  "DataUploadServiceDequeueInterval": 9999,
  "DataUploadServiceCleanupTimeout": 99999,
  "StatUnitAnalysisRules": {
    "MandatoryFields": {
      "CheckContactPerson": true,
      "CheckDataSource": true,
      "CheckName": true,
      "CheckRegistrationReason": true,
      "CheckShortName": true,
      "CheckStatus": true,
      "CheckTelephoneNo": true,
      "CheckLegalUnitOwner": true
    },
    "Connections": {
      "CheckRelatedActivities": true,
      "CheckRelatedLegalUnit": true,
      "CheckAddress": true
    },
    "Orphan": {
      "CheckRelatedEnterpriseGroup": true
    },
    "Duplicates": {
      "CheckName": true,
      "CheckStatIdTaxRegId": true,
      "CheckExternalId": true,
      "CheckShortName": true,
      "CheckTelephoneNo": true,
      "CheckAddressId": true,
      "CheckEmailAddress": true,
      "CheckContactPerson": true,
      "CheckOwnerPerson": true,
      "MinimalIdenticalFieldsCount": 2
    }
  },
  "Localizations": [ "ru-RU", "en-US", "ky-KG" ],
  "DefaultLocalization": "en-US",

  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "System": "Information",
      "Microsoft": "Information"
    }
  }
}