{
  "version": "1.0.0-*",
  "buildOptions": {
    "emitEntryPoint": true,
    "copyToOutput": {
      "includeFiles": [ "appsettings.json" ]
    }
  },

  "dependencies": {
    "Microsoft.EntityFrameworkCore.InMemory": "1.1.0",
    "Microsoft.NETCore.App": {
      "type": "platform",
      "version": "1.1.0"
    },
    "nscreg.Data": "1.0.0-*",
    "Npgsql.EntityFrameworkCore.PostgreSQL": "1.1.0",
    "PeterKottas.DotNetCore.WindowsService": "1.0.18"
  },

  "frameworks": {
    "netcoreapp1.1": {
      "imports": [
        "dotnet5.6",
        "portable-net45+win8"
      ]
    }
  },

  "publishOptions": {
    "include": [
      "appsettings.json"
    ]
  },
  "tooling": {
    "defaultNamespace": "nscreg.DataUploadService"
  }
}
